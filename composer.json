{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
	 "symfony/symfony": "2.6.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "1.0.*@dev",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0,>=3.0.12",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "jms/security-extra-bundle": "1.*",
        "jms/di-extra-bundle": "1.*",
        "kriswallsmith/assetic": "1.1.*@dev",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "~1.3",
	 "ideup/simple-paginator-bundle": "dev-master",
        "knplabs/knp-menu": ">=1.1.0,<3.0.0",
        "knplabs/knp-menu-bundle":">=1.1.0,<3.0.0",
	 "knplabs/knp-snappy-bundle": "dev-master",	
        "genemu/form-bundle": "2.3.*@dev",
        "sonata-project/core-bundle": "~2.2",
	 "sonata-project/media-bundle": "~2.2",
        "sonata-project/admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2",
        "sonata-project/jquery-bundle": "1.8.x-dev",
        "sonata-project/cache-bundle": "~2.1",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/exporter": "1.3.1",
        "sonata-project/doctrine-extensions": "1.0.1",
        "knplabs/gaufrette": "dev-master",
        "sonata-project/block-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "javiereguiluz/easyadmin-bundle": "dev-master",
        "nzo/url-encryptor-bundle": "~1.0",
        "stfalcon/tinymce-bundle": "dev-master"

     
    
        
    },

    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    }
}
