
{% extends 'BackendBundle:Extranet:perfil.html.twig' %}
{% block main %}
<div id="main">
<div id="main_content">
<div id="main_content_controls">
<h1>Comitè</h1>
<div id="main_content_button"><a href="{{ path('extranet_comite_nou')}}" class="button_orange"><img src="{{asset('uploads/images/icon_plus.png')}}" width="16" height="16" alt="plus" />  Afegir comitè</a></div>
<div id="main_content_pagination">
{% if paginador.currentPage > 1 %}
<a href="{{ path('extranet_comite', { 'page': paginador.firstPage })}}" onmouseout="MM_swapImgRestore()" onmouseover="MM_swapImage('first','','{{asset('uploads/images/icon_first_activo.png')}}',1)"><img src="{{asset('uploads/images/icon_first_reposo.png')}}" alt="first" width="16" height="11" id="first" />
<a href="{{ path('extranet_comite', { 'page': paginador.previousPage}) }}" onmouseout="MM_swapImgRestore()" onmouseover="MM_swapImage('previous','','{{asset('uploads/images/icon_previus_activo.png')}}',1)">
<img src="{{asset('uploads/images/icon_previus_reposo.png')}}" alt="previous" width="9" height="11" id="previous" /></a>
{% else %}
<img src="{{asset('uploads/images/icon_first_reposo.png')}}" alt="first" width="16" height="11" id="first" />
<img src="{{asset('uploads/images/icon_previus_reposo.png')}}" alt="previous" width="9" height="11" id="previous" />
{% endif %}
{% for page in paginador.minPageInRange..paginador.maxPageInRange %}
{% if page == paginador.currentPage %}
{{ page }}
{% else %}
<a href="{{ path('extranet_comite', { 'page': page }) }}">{{ page
}}</a>
{% endif %}
{% endfor %}
{% if paginador.currentPage < paginador.lastPage %}
<a href="{{ path('extranet_comite', { 'page': paginador.nextPage })}}" onmouseout="MM_swapImgRestore()" onmouseover="MM_swapImage('next','','images/icon_next_activo.png',1)">
<img src="{{asset('uploads/images/icon_next_reposo.png')}}" alt="next" width="9" height="11" id="next" /></a> 
<a href="{{ path('extranet_comite', { 'page': paginador.lastPage })}}" onmouseout="MM_swapImgRestore()" onmouseover="MM_swapImage('last','','{{asset('uploads/images/icon_last_activo.png')}}',1)">
<img src="{{asset('uploads/images/icon_last_reposo.png')}}" alt="last" width="16" height="11" id="last" /></a>
{% else %}
<img src="{{asset('uploads/images/icon_next_reposo.png')}}" alt="next" width="9" height="11" id="next" />
<img src="{{asset('uploads/images/icon_last_reposo.png')}}" alt="last" width="16" height="11" id="last" />
{% endif %}
</div>
</div>
<div id="main_content_tables">
<br>
<br>
<table cellspacing="0">
    <tr><th>Nom</th><th></th></tr>
   {% for comite in comites %}
    <tr><td>{{ comite.Nom}}</td><td><a href="{{ path('extranet_comite_editar', {'id': comite.id })}}" class="button_purple_small">Editar</a></td></tr>
   {% endfor %}
    </table>
</div>
</div>
</div>
{% endblock %}

