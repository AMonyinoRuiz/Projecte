  <div id="{{class1}}" align="{{align1}}"><iframe src="{{activitat.Text1}}" width="{{llarg1}}" height="281" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe></div>
  {% endif %}
  {% if activitat.TipusCamp1== 'Texte'  %}
